// defining backend layout for news detail page
mod {
	web_layout {
		BackendLayouts {

			news {
				title = News Detail Template
				description = Website News Detail BE template
				config {
					backend_layout {
						colCount = 1
						rowCount = 1
						rows {
							1 {
								columns {
									1 {
										name = Content
										colPos = 0
									}
								}
							}
						}
					}
				}
			}

		}
	}
}
