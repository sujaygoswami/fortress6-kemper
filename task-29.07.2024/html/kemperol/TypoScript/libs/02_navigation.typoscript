lib.navigation.language = COA
lib.navigation.language {
    wrap = <li class="has-sub-item dropdown">|</li>

    10 = TEXT
    10 {
        value = Deutsch
        wrap = <a href="javascript:void(0)">|</a><span class="dropdown-toggle" data-toggle="dropdown"></span>
    }

    20 = HMENU
    20 {
        special = language
        special.value = 0,1
        special.normalWhenNoLanguage = 0

        addQueryString = 1
        addQueryString.method = GET
        addQueryString.exclude = L,cHash

        wrap = <div class="dropdown-menu">|</div>

        1 = TMENU
        1 {
            NO = 1
            NO {
                stdWrap.htmlSpecialChars = 1
                stdWrap.htmlSpecialChars.preserveEntities = 1
                stdWrap {
                    setCurrent = Deutsch||English
                    current = 1
                }
                ATagParams = class="dropdown-item"
            }

            # Current language selected:
            ACT < .NO
            ACT {
                ATagParams = class="dropdown-item active"
            }
        }
    }
}

