#########################
#### Main navigation ####
#########################
lib.navigation.main = HMENU
lib.navigation.main {

    entryLevel = 0

    1 = TMENU
    1 {
        noBlur = 1
        expAll = 1
        wrap = |
        NO = 1
        NO {
            wrapItemAndSub = <li class="has-sub-item sub-item-closed">|</li>
            doNotLinkIt = 1
            stdWrap.cObject = COA
            stdWrap.cObject {
                10 = TEXT
                10 {
                    field = nav_title // title
                    typolink.parameter.field = uid
                }

                20 = CONTENT
                20 {
                    table = tt_content
                    select {
                        pidInList.field = uid
                        where = colPos = 1
                    }
                }
            }
        }

        CUR < .NO
        CUR {
            wrapItemAndSub = <li class="has-sub-item sub-item-closed active current">|</li>
        }

        ACT < .NO
        ACT {
            wrapItemAndSub = <li class="has-sub-item sub-item-closed active">|</li>
        }
    }
}



################################
#### Application navigation ####
################################
lib.navigation.application = HMENU
lib.navigation.application {
    special = list
    special.value.current = 1

    1 = TMENU
    1 {
        noBlur = 1
        expAll = 1
        NO = 1
        NO {
            wrapItemAndSub = <div class="col-md-6 my-col">|</div>
            doNotLinkIt = 1
            stdWrap.cObject = COA
            stdWrap.cObject {
                stdWrap.typolink.parameter.field = uid
                stdWrap.typolink.ATagParams = class="full-height"
                wrap = <div class="menu-item-holder-wrap full-height"><div class="table-wrap"><div class="table-cell middle-line"><div class="menu-item-holder menu-with-icon">|</div></div></div></div>
                              
                10 = FILES
                10 {
                    references { 
                        table = pages 
                        uid.field = uid
                        fieldName = media 
                    }
                    begin = 0
                    maxItems = 1
                    renderObj = COA 
                    renderObj { 
                        10 = IMAGE 
                        10 { 
                            file.import.data = file:current:publicUrl
                            file.maxW = 50
                            file.maxH = 40
                            stdWrap.wrap = <span>|</span>
                        }
                    }
                }

                20 = TEXT
                20 {
                    field = nav_title // title
                    wrap = <i>|</i>
                }
            }
        }

        ACT < .NO
        ACT {
            stdWrap.cObject.stdWrap.typolink.ATagParams = class="full-height active"
        }
    }
}

